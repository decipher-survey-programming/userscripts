{
  "manifest_version": 2,

  "name": "Decipher Keyboard Shortcuts",
  "description": "This extension provides keyboard shortcuts for navigating around surveys",
  "version": "1.1",

  "icons": { 
          "128": "Deciphericon.png" },
  
    "content_scripts": [
    {
      "matches": ["https://v2.decipherinc.com/*","http://v2.decipherinc.com/*"],      
      "js": ["Shortcuts.js"]
    }
  ],
  
  "permissions": [
    "storage"
  ]
}
